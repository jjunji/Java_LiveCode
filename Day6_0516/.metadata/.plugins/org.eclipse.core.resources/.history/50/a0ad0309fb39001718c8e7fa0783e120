package com.jihoon.java.city;

public class People {
						//속성은 private로 바꾼다. 
	public String name ;
	private String gender ;
	private int age ;
	private String skinColor ;
	private String haircolor ;
	private int height ;
	private int weight;
	
	public void move(String direction){
		System.out.println(direction + " 방향으로 움직인다.");
	}

}
