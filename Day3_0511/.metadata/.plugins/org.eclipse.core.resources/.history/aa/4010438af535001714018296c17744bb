package com.jihoon.java.fibo;

public class test {

	public static void main(String[] args) {
		
		int array[] = {0};
		
		array[0] = 0;
		array[1] = 1;
		
		for(int i=1; i<10; i++){
			array[i+1] = array[i-1] + array[i];
			System.out.println(array[i]);
			array[i-1] = array[i];
			array[i] = array[i+1];
			
		}
	}

}
